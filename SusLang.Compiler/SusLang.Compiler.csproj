<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <RootNamespace>SusLang</RootNamespace>
        <Company>zenonet</Company>
        <Authors>zenonet</Authors>
        <PackageVersion>1.0.0</PackageVersion>
        <RepositoryUrl>https://github.com/zenonet/SusLang</RepositoryUrl>
        <LangVersion>10</LangVersion>
    </PropertyGroup>

    <ItemGroup>
      <Compile Remove="bin\**" />
      <Compile Remove="obj\**" />
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Remove="bin\**" />
      <EmbeddedResource Remove="obj\**" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="bin\**" />
      <None Remove="obj\**" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="SoftCircuits.OrderedDictionary" Version="2.1.0" />
    </ItemGroup>

</Project>
